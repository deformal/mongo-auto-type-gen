export type Orders = {
  _id: string;
  added_tax_amount_after_terms: number;
  createdAt: ISODateString;
  delivery_fees: number;
  expected_final_delivery_date: ISODateString;
  expected_initial_delivery_date: ISODateString;
  invoices: Invoices[];
  merge_shipments: boolean;
  order_entity: OrderEntity;
  order_note: string;
  order_payment_term: OrderPaymentTerm;
  order_products_after_terms_total_discount: number;
  order_sub_total_after_terms: number;
  order_total_after_discount_after_promo_after_tax_after_delivery_fees_after_credit_balance_deduction: number;
  order_total_after_terms_after_discount_after_promo_after_tax_after_delivery_fees: number;
  order_total_tax_after_terms: number;
  payment_method: PaymentMethod;
  ref_id: number;
  sale_order_number: string;
  shipment_ids: (string)[];
  shipments: Shipments[];
  shipping_method_ref_id: number;
  status: string;
  updatedAt: ISODateString;
  use_credit_balance: boolean;
  use_payment_term: boolean;
};

export type Invoices = {
  _id: string;
  delivery_charge: number;
